/div> -->\n</div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    data(){\n        return {\n            response: \"\",\n        }\n    },\nmounted: function() {\n this.getArticles();\n},\n    name: 'ShowDataBase',\n    methods:{\n        getArticles: async function()  {\n            const endpoint = \"/api/v2/bookdata/\";\n            this.response = await axios.get(endpoint);\n        },\n        async deleteData(){\n            const endpoint = \"/api/v2/bookdata/1\";\n            await axios.delete(endpoint)\n            .then((response) => {\n                this.response = response.data;\n                this.loading = false;\n            })\n            .catch((err) => {\n            this.loading = false;\n            console.log(err);\n            });\n        }\n    }\n    }\n</script>"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,QAAQ,EAAE;IACd;EACJ,CAAC;EACLC,OAAO,EAAE,SAAAA,CAAA,EAAW;IACnB,IAAI,CAACC,WAAW,CAAC,CAAC;EACnB,CAAC;EACGC,IAAI,EAAE,cAAc;EACpBC,OAAO,EAAC;IACJF,WAAW,EAAE,eAAAA,CAAA,EAAkB;MAC3B,MAAMG,QAAO,GAAI,mBAAmB;MACpC,IAAI,CAACL,QAAO,GAAI,MAAMF,KAAK,CAACQ,GAAG,CAACD,QAAQ,CAAC;IAC7C,CAAC;IACD,MAAME,UAAUA,CAAA,EAAE;MACd,MAAMF,QAAO,GAAI,oBAAoB;MACrC,MAAMP,KAAK,CAACU,MAAM,CAACH,QAAQ,EAC1BI,IAAI,CAAET,QAAQ,IAAK;QAChB,IAAI,CAACA,QAAO,GAAIA,QAAQ,CAACD,IAAI;QAC7B,IAAI,CAACW,OAAM,GAAI,KAAK;MACxB,CAAC,EACAC,KAAK,CAAEC,GAAG,IAAK;QAChB,IAAI,CAACF,OAAM,GAAI,KAAK;QACpBG,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;MAChB,CAAC,CAAC;IACN;EACJ;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}